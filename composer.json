{
    "name": "alphao/cover",
    "type": "library",
    "description": "A simple PHP Code",
    "keywords": [
        "App",
        "Cover"
    ],
    "homepage": "https://github.com/App/Cover",
    "license": "MIT",
    "authors": [
        {
            "name": "Alpha Olomi",
            "email": "hello@alphaolomi.com",
            "homepage": "https://github.com/alphaolomi",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "8.4.3",
        "squizlabs/php_codesniffer": "3.5.2"
    },
    "autoload": {
        "psr-4": {
            "App\\Cover\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Cover\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
